<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.partner.view.homepage">

    <TextView
        android:layout_width="450dp"
        android:layout_height="180dp"
        android:layout_marginBottom="817dp"
        android:background="#0A7F8F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="392dp"
        android:layout_marginBottom="957dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icone" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="310dp"
        android:layout_marginBottom="960dp"
        android:text="PARTNER"
        android:textColor="#ECE4E4"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonhome"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="391dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="947dp"
        android:backgroundTint="@color/bluHome"
        android:foregroundTint="#0A7F8E"
        app:icon="@drawable/baseline_home_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonatividades"
        android:layout_width="77dp"
        android:layout_height="70dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="341dp"
        android:layout_marginBottom="838dp"
        android:background="@null"
        android:backgroundTint="#3CA7B6"
        app:icon="@drawable/tasks_all_svgrepo_com"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonagenda"
        android:layout_width="77dp"
        android:layout_height="70dp"
        android:layout_marginStart="176dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="195dp"
        android:layout_marginBottom="838dp"
        android:background="@null"
        android:backgroundTint="#3CA7B6"
        app:icon="@drawable/baseline_book_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonperfil"
        android:layout_width="77dp"
        android:layout_height="70dp"
        android:layout_marginStart="324dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="838dp"
        android:background="@null"
        android:backgroundTint="#3CA7B6"
        app:icon="@drawable/person_svgrepo_com"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>